{% extends "base.html" %}
{% block body-class %}about{% endblock %}
{% block content %}
  <h3>{{ page.title }}</h3>
  <h4>{{ page.subhead }}</h4>
  {{ page.primary_content_rendered_markup }}
  {{ page.fine_print_rendered_markup }}
{% endblock %}
{% block sidebar %}
	{% if page.secondary_content %}
		<div class="module">
	  	{{ page.secondary_content_rendered_markup }}
	  </div>
	 <div class="floating-logo-wrapper"><div class="floating-logo-left"></div></div>
	 {% include "snippets/newsletter.html" %}
  {% endif %}
{% endblock %}
