{% extends "base.html" %}

{% block title %}Projects for {{ year }}{% endblock %}

{% block content %}
  <h2>Projects for {{ year }}</h2>
  <ul>
    {% for project in project_list %}
      <li><a href="{{ project.get_absolute_url }}">{{ project.title }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}

{% block sidebar %}
  <h3>{{ year }}</h3>
  <ul>
    {% for month in date_list %}
      <li><a href="{% url project_index %}{{ year }}/{{ month|date:"b" }}/">{{ month|date:"F" }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}