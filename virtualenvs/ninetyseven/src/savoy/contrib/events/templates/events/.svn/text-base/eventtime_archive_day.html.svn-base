{% extends "base.html" %}

{% block title %}Events for {{ day|date:"F, jS, Y" }}{% endblock %}

{% block content %}
  <h2>Events for {{ day|date:"F, jS, Y" }}</h2>
  <ul>
    {% for eventtime in eventtime_list %}
      <li><a href="{{ eventtime.get_absolute_url }}">{{ eventtime.event.title }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}