{% extends "base.html" %}

{% block title %}Photos for {{ year }}{% endblock %}

{% block content %}
  <h2>Photos for {{ year }}</h2>
  <ul>
    {% for photo in photo_list %}
      <li><a href="{{ photo.get_absolute_url }}">{{ photo.title }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}

{% block sidebar %}
  <h3>{{ year }}</h3>
  <ul>
    {% for month in date_list %}
      <li><a href="{% url photo_index %}{{ year }}/{{ month|date:"b" }}/">{{ month|date:"F" }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}